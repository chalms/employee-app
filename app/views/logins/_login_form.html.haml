.form.form-inline
  %form{:role => 'form', :method => 'post', :action => '/logins'}
    %p= link_to "Not a member?", '/'
    .group
      .input-prepend.first
        %span.add-on
          %i.icon-envelope
        %input.span2.form-control{:type => "text", :name => "email", :placeholder => "Email"}
    .group
      .input-prepend
        %span.add-on
          %i.icon-key
        %input.span2.form-control{:type => "password", :name => "password", :placeholder => "Password"}
    %input.btn.btn-default{:style => "visibility: hidden;", :type => "submit"}